    <script type='text/javascript'>
    function shutdown_action(action, pass="", message="", time=0)
    {
    var pass_str = ""
    if (pass!=""){pass_str="&password="+pass}
    window.location.replace('/?action='+action+''+ pass_str+'&message='+message+'&time='+time)
    }
